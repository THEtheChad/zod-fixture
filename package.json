{
	"name": "zod-fixture",
	"version": "0.0.0",
	"description": "Creating fixtures based on zod schemas",
	"packageManager": "npm@9.5.0",
	"scripts": {
		"start": "vite",
		"test": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"build": "vite build",
		"lint": "eslint . --max-warnings 0",
		"pre-commit": "npm run lint && npm run test:coverage",
		"prepare": "git config core.hookspath .githooks"
	},
	"files": [
		"dist"
	],
	"main": "./dist/zod-fixture.umd.js",
	"module": "./dist/zod-fixture.es.js",
	"exports": {
		".": {
			"import": "./dist/zod-fixture.es.js",
			"require": "./dist/zod-fixture.umd.js"
		}
	},
	"types": "./dist/index.d.ts",
	"type": "module",
	"keywords": [
		"zod",
		"fixtures"
	],
	"author": "Tim Deschryver",
	"license": "MIT",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.30.7",
		"@typescript-eslint/parser": "^5.30.7",
		"@vitest/ui": "^0.31.0",
		"c8": "^7.12.0",
		"eslint": "^8.20.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"prettier": "^2.7.1",
		"typescript": "^5.0.4",
		"vite": "^3.2.6",
		"vite-plugin-dts": "^1.4.1",
		"vitest": "^0.31.0"
	},
	"dependencies": {
		"zod": "^3.21.4"
	},
	"imports": {
		"@/*": "./src/*"
	}
}
